@using React.Web.Mvc
@using React.RenderFunctions

@{
	ViewBag.Title = "Home Page";

	var styledComponentsFunctions = new StyledComponentsFunctions();
}

@Html.React("HomeComponents.EntryPoint", new { text = ViewBag.MessageText }, renderFunctions: styledComponentsFunctions)

@{ 
	ViewBag.RenderedStyles = styledComponentsFunctions.RenderedStyles;
}